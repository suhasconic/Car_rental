{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/api/*",
            "/assets/*"
        ]
    },
    "routes": [
        {
            "route": "/api/*",
            "allowedRoles": [
                "anonymous"
            ]
        }
    ],
    "globalHeaders": {
        "Cache-Control": "no-cache"
    },
    "mimeTypes": {
        ".json": "application/json",
        ".js": "application/javascript",
        ".css": "text/css"
    }
}